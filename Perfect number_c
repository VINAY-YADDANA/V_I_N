#include <stdio.h>
int main()
{
	int n,sum=0,r;
	printf("Enter a num:");
	scanf("%d",&n);
	for(int i=1;i<n;i++)
	{
	    r=n%i;
		if(r==0)
        {
          sum=sum+i;
        }
        //printf("%d",sum);
	}
	//printf("%d",sum);
    if(sum==n)
    {
       printf("PERFECT");
    }
    else
    {
        printf("NOT PERFECT");

    }


}
